Algoritmo "consumo"

Var

       Dt, Ct, consumo:real

Inicio

       Escreva("Digite a distância total percorrida: ")
       leia(Dt)
       Escreva("Digite o combustivel total gasto: ")
       leia(Ct)
       
       consumo <- Dt/Ct
       
       Escreva("O consumo médio desse carro é de: ",consumo:2:3)
       


Fimalgoritmo